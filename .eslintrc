{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/prefer-stateless-function": 0,
    "react/jsx-filename-extension": 0,
    "class-methods-use-this": 0,
    "no-return-assign": 0,
    "no-param-reassign": 0,
    "camelcase": 0,
    "indent": [
      2,
      2
    ],
    "max-len": [
      "error",
      120
    ]
  },
  "globals": {
    "document": true,
    "window": true
  }
}
